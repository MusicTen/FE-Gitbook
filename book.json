{
    "title": "MusicTen 知识拾遗",
    "author": "MusicTen",
    "description": "select * from learn",
    "language": "zh-hans",
    "gitbook": "3.2.3",
    "structure": {
        "readme": "README.md"
    },
    "plugins": [
        "github",
        "favicon",
        "splitter",
        "page-toc-button",
        "pageview-count",
        "expandable-chapters",
        "back-to-top-button",
        "tbfed-pagefooter"
    ],
    "pluginsConfig": {
        "github": {
            "url": "https://github.com/MusicTen"
        },
        "favicon": {
            "shortcut": "assets/favicon.ico",
            "bookmark": "assets/favicon.ico"
        },
        "page-toc-button": {
            "maxTocDepth": 2,
            "minTocSize": 2
   		},
        "tbfed-pagefooter": {
            "copyright": "Copyright &copy MusicTen 2019",
            "modify_label": "Updated at",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        }
    }
}